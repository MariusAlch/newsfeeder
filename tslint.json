{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "member-access": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "no-reference": false,
        "member-ordering": false,
        "variable-name": false
    },
    "rulesDirectory": []
}