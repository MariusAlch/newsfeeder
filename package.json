{
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit --project tsconfig.json && tsc --noEmit --project tsconfig.server.json && npm run lint"
    }
  },
  "scripts": {
    "dev": "export NODE_ENV=development && concurrently \"nodemon server/index.ts\" \"watch 'npm run build-widget' lib shared\"",
    "build": "export NODE_ENV=production && next build && tsc --project tsconfig.server.json && npm run build-widget",
    "build-widget": "parcel build --no-source-maps ./lib/components/widget/WidgetScript.tsx && mv ./dist/WidgetScript.js ./static/WidgetScript.js",
    "start": "NODE_ENV=production node .next/production-server/server/index.js",
    "lint": "tslint './lib/**/*.ts{,x}' './server/**/*.ts{,x}' './pages/**/*.ts{,x}' './shared/**/*.ts{,x}' './landing/**/*.ts{,x}'"
